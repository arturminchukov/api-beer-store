{
    "name": "api-beer-store",
    "version": "1.0.0",
    "description": "api for beer-store",
    "main": "app.js",
    "scripts": {
        "start": "node app.js",
        "debug": "node --inspect-brk app.js",
        "lint": "./node_modules/.bin/eslint db/ app.js domain/ application/ web/",
        "lint-fix": "./node_modules/.bin/eslint --fix db/ app.js domain/ application/ web/",
        "migrate": "./node_modules/.bin/sequelize db:migrate",
        "create": "./node_modules/.bin/sequelize db:create",
        "precommit": "sh pre-commit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/arturminchukov/api-beer-store.git"
    },
    "author": "Artur Minchukov",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/arturminchukov/api-beer-store/issues"
    },
    "homepage": "https://github.com/arturminchukov/api-beer-store#readme",
    "dependencies": {
        "axios": "^0.18.0",
        "body-parser": "^1.18.3",
        "express": "^4.16.4",
        "pg": "^7.8.0",
        "pg-hstore": "^2.3.2",
        "sequelize": "^4.42.0",
        "sequelize-cli": "^5.4.0"
    },
    "devDependencies": {
        "eslint": "^5.12.1",
        "husky": "^1.3.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run precommit"
        }
    }
